    {
      "parameters": {
        "functionCode": "// Groq cevabÄ±nÄ± JSON'a Ã§evir ve alanlarÄ± normalize et\nconst content = $json.choices?.[0]?.message?.content || '';\nlet data;\ntry {\n  data = JSON.parse(content);\n} catch (e) {\n  // JSON parse hatasÄ± varsa, baÅŸtaki gereksiz karakterleri ve sondaki fazlalÄ±klarÄ± temizle\n  const cleaned = content.replace(/^[^{]*{/, '{').replace(/}[^}]*$/, '}');\n  try {\n    data = JSON.parse(cleaned);\n  } catch (e2) {\n    throw new Error('Metadata JSON parse edilemedi');\n  }\n}\n\n// YouTube iÃ§in etiketleri dÃ¼zgÃ¼n ÅŸekilde iÅŸle (her etiket 30 karakterden uzun olamaz ve 500 etiketten fazla olamaz)\nfunction cleanTag(tag) {\n  return tag.replace(/[^a-zA-Z0-9]/g, '').substring(0, 30);\n}\n\nlet youtubeTags = [];\nif (Array.isArray(data.tags)) {\n  youtubeTags = data.tags.map(tag => cleanTag(typeof tag === 'string' ? tag : String(tag))).filter(tag => tag.length > 0).slice(0, 500);\n}\n\n// Keywords ve hashtags'leri de etiketlere ekle (duplicate'leri kaldÄ±rarak)\nconst allTags = new Set(youtubeTags);\nif (Array.isArray(data.keywords)) {\n  data.keywords.forEach(keyword => {\n    const cleanedKeyword = cleanTag(typeof keyword === 'string' ? keyword : String(keyword));\n    if (cleanedKeyword.length > 0) {\n      allTags.add(cleanedKeyword);\n    }\n  });\n}\nif (Array.isArray(data.hashtags)) {\n  data.hashtags.forEach(hashtag => {\n    const cleanedHashtag = cleanTag(hashtag.replace('#', ''));\n    if (cleanedHashtag.length > 0) {\n      allTags.add(cleanedHashtag);\n    }\n  });\n}\n\n// Fallback deÄŸerler - eÄŸer AI boÅŸ dÃ¶ndÃ¼rÃ¼rse\nconst fallbackTitle = 'ğŸš€ Ä°nanÄ±lmaz Ä°Ã§erik - KaÃ§Ä±rma! | Viral Video 2024';\nconst fallbackDescription = 'ğŸ¬ Bu muhteÅŸem videoyu izlediÄŸiniz iÃ§in Ã§ok teÅŸekkÃ¼r ederiz!\\n\\nğŸ”¥ Bu videoda sizi bekleyen harika iÃ§erikler:\\nâœ… Profesyonel kalitede eÄŸitici iÃ§erik\\nâœ… SÃ¼rÃ¼kleyici ve eÄŸlenceli anlar\\nâœ… HayatÄ±nÄ±zÄ± deÄŸiÅŸtirecek deÄŸerli bilgiler\\nâœ… Pratik ipuÃ§larÄ± ve Ã¶neriler\\n\\nğŸ’¡ Daha fazla kaliteli ve Ã¶ÄŸretici iÃ§erik iÃ§in:\\nğŸ”” ABONE OLMAYI unutmayÄ±n (ÃœCRETSÄ°Z!)\\nğŸ‘ Videoyu BEÄENÄ°N - Bu bize Ã§ok yardÄ±mcÄ± oluyor!\\nğŸ’¬ DÃ¼ÅŸÃ¼ncelerinizi YORUMLARDA paylaÅŸÄ±n\\nğŸ“¤ Sevdiklerinizle PAYLAÅIN\\n\\nğŸ† Bizi takip edin ve hiÃ§bir iÃ§eriÄŸi kaÃ§Ä±rmayÄ±n!\\n\\nğŸ·ï¸ #YouTube #EÄŸitim #Viral #Trend #Kaliteli #Ã–ÄŸretici #TÃ¼rkÃ§e #2024 #Ä°zlemeye DeÄŸer #MuhteÅŸem';\nconst fallbackTags = ['YouTube', 'Egitim', 'Viral', 'Trend', 'Kaliteli', 'Ogretici', 'Turkce', '2024', 'IzlemeyeDeger', 'Muhtesem', 'Harika', 'Bilgi', 'Ipucu', 'Pratik', 'Faydali', 'Guncel', 'Populer', 'Kesfet', 'Onerilen', 'Ilginc', 'Basarili', 'Motivasyon', 'Gelisim', 'Yenilik', 'Teknoloji', 'Hayat', 'Degisim', 'Ilham', 'Rehber'];\n\nconst result = {\n  youtube_metadata: {\n    title: (data.title && data.title.trim()) || fallbackTitle,\n    description: (data.description && data.description.trim()) || fallbackDescription,\n    tags: Array.from(allTags).length > 0 ? Array.from(allTags).slice(0, 500) : fallbackTags,\n    keywords: Array.isArray(data.keywords) && data.keywords.length > 0 ? data.keywords : fallbackTags,\n    categoryId: (typeof data.categoryId === 'number' || /^\\d+$/.test(String(data.categoryId))) ? Number(data.categoryId) : 24,\n    hashtags: Array.isArray(data.hashtags) && data.hashtags.length > 0 ? data.hashtags : ['#YouTube', '#Video', '#Ä°Ã§erik', '#Trend', '#Viral', '#TÃ¼rkÃ§e', '#TR', '#Kaliteli', '#EÄŸlence', '#Ã–ÄŸretici'],\n    language: data.language || 'tr'\n  }\n};\nreturn [result];"
      },
